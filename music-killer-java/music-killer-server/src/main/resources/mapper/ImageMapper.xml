<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xyz.jdynb.mapper.ImageMapper">

    <resultMap id="BaseResultMap" type="xyz.jdynb.entity.Image">
            <id property="id" column="id" />
            <result property="name" column="name" />
            <result property="size" column="size" />
            <result property="md5" column="md5" />
            <result property="sha1" column="sha1" />
            <result property="mime" column="mime" />
            <result property="url" column="url" />
            <result property="ext_id" column="ext_id" />
            <result property="create_time" column="create_time" />
    </resultMap>

    <sql id="Base_Column_List">
        id,name,size,md5,sha1,mime,
        url,ext_id,create_time
    </sql>
</mapper>
